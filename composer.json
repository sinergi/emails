{
  "name": "smart/emailqueue",
  "type": "library",
  "description": "Email Queue supporting multiple worker, ORM and email sending platforms",
  "keywords": [
    "Email",
    "Queue",
    "Gearman",
    "RabbitMQ",
    "Doctrine",
    "Postmark"
  ],
  "homepage": "https://github.com/smart-io/emailqueue",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Bull",
      "email": "me@gabrielbull.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "doctrine/orm": "~2",
    "cocur/slugify": "~1",
    "psr/log": "~1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4",
    "sinergi/gearman": "<1",
    "videlalvaro/php-amqplib": "2.5.*",
    "znarkus/postmark": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Smart\\EmailQueue\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Smart\\EmailQueue\\Tests\\": ["tests/EmailQueue/Tests"]
    }
  }
}
